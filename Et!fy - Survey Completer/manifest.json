{
  "manifest_version": 3,
  "name": "Et!fy - Survey Completer",
  "description": "ET!fy fills in and submits ETLab faculty evaluations automatically with a simple click thus saving you time and effort.",
  "version": "1.2",
  "icons": {
      "16": "./icons/icon16.png",
      "48": "./icons/icon48.png",
      "128": "./icons/icon128.png"
    },
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "./icons/icon16.png",
      "48": "./icons/icon48.png",
      "128": "./icons/icon128.png"
    }
  }
}